<?xml version="1.0" encoding="utf-8"?>
<Icon version="1" frameRate="60" width="384" height="384" screenWidth="1080" resDensity="480">
	<!-- control -->
	<ExternalCommands>
		<!-- 首次使用主题、解锁、返回桌面连续按home键切屏时，将动画播放到最后一帧 -->
		<Trigger action="init,present,back_home_cancel">
			<FolmeCommand target="icon" states="'state_end'" command="setTo" config="'state_config'" />
		</Trigger>
		<!-- 退出APP返回桌面的瞬间 -->
		<Trigger action="back_home_start">
			<FolmeCommand target="icon" states="'state_init'" command="setTo" config="'state_config'"/>
		</Trigger>
		<!-- 退出APP回到桌面 -->
		<Trigger action="back_home_finish">
			<FolmeCommand target="icon" states="'state_init','state_end'" command="fromTo" config="'state_config'" />
		</Trigger>
	</ExternalCommands>
	<!-- module -->
	<FolmeState name="state_init" x="93" rotation="200*#ratio" />
	<FolmeState name="state_end"  x="93" rotation="0" />
	<FolmeConfig name="state_config" property="x,rotation" ease="-2,0.9,1" fromSpeed="0"/>
	<!-- view -->
	<Group>
		<Image name="icon"  src="icon.png"  x="93" y="93" folmeMode="true" pivotX="82" pivotY="82"/>
	</Group>
</Icon>